generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

model User {
  id              String      @id @default(cuid())
  telegramId      String      @unique
  username        String?
  createdAt       DateTime    @default(now())
  updatedAt       DateTime    @updatedAt
  quote           String
  quoteUsername   String
  quoteTime       DateTime
}